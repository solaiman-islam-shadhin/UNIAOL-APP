{
  "expo": {
    "name": "UniSol",
    "slug": "UniSol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-icon.png",
    "scheme": "unisol",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-icon.png"
        
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
         
        }
      ],
      "expo-font"
      


    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
